{
  "name": "yacht-book-app",
  "version": "1.0.0",
  "description": "this is a web app that make you be able to book a yacht and a trip in just single booking",
  "main": "app.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "bulid": "cd client && npm run build",
    "client": "cd client && npm start",
    "dev": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\""
